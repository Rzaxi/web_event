{
  "json.schemas": [],
  "files.associations": {
    "*.json": "json"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "json.validate.enable": false,
  "json.schemaDownload.enable": false,
  "json.maxItemsComputed": 5000,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true
  }
}